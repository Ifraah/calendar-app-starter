{
  "name": "hygiene_compliance",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@personare/react-freshdesk-widget": "^0.3.0",
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.2.0",
    "bootstrap": "4.0.0-beta.2",
    "create-react-class": "^15.6.2",
    "font-awesome": "^4.7.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.5.0",
    "javascript-natural-sort": "^0.7.1",
    "moment": "^2.19.2",
    "moment-duration-format": "^2.2.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-apollo": "^2.0.1",
    "react-csv": "^1.0.8",
    "react-dates": "^15.1.0",
    "react-dom": "^16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.4.1",
    "react-moment-proptypes": "^1.5.0",
    "react-newline-to-break": "^1.0.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-named-routes": "^0.0.19",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.1",
    "react-virtualized": "^9.12.0",
    "reactstrap": "^5.0.0-alpha.3",
    "redux-persist": "^5.9.1",
    "redux-storage": "^4.1.2",
    "redux-storage-engine-localstorage": "^1.1.4",
    "victory": "^0.25.6"
  },
  "scripts": {
    "cci-homepage": "node .circleci/update_package_homepage.js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "csslint": "stylelint **/*.scss",
    "csslint:fix": "stylelint **/*.scss --fix",
    "start-app": "npm-run-all -p watch-css start-js",
    "start": "npm-run-all -p start-app start-mock-server",
    "start:local": "REACT_APP_API_ENDPOINT=http://localhost:8888/graphql yarn start-app",
    "start:prod": "REACT_APP_API_ENDPOINT=https://api.swipesense.com/graphql yarn start-app",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run build-css && react-scripts test --env=jsdom",
    "start-mock-server": "nodemon mockServer/bin/www",
    "eject": "react-scripts eject",
    "storybook": "npm-run-all -p watch-css start-storybook",
    "start-storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-backgrounds": "^3.0.0",
    "@storybook/addon-info": "^3.2.14",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "codecov": "^3.0.0",
    "cors": "^2.8.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express": "^4.16.2",
    "graphql-tools": "^2.13.0",
    "jest-enzyme": "^4.0.1",
    "mockdate": "^2.0.2",
    "nodemon": "^1.12.5",
    "prettier": "^1.7.4",
    "react-storybook-decorator-background": "^1.0.5",
    "react-test-renderer": "^16.0.0",
    "styled-components": "^2.2.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  },
  "engines": {
    "yarn": "1.3.2",
    "node": "8.7.0"
  }
}
